
@{
    ViewBag.Title = "UpdateUser";
    Layout = "~/Areas/Admin/Views/Shared/AdminLayout.cshtml";
}


<secton class="content-header">
    <ol class="breadcrumb float-sm-left">
        <li class="breadcrumb-item"><a>Admin</a></li>
        <li class="breadcrumb-item"><a>User</a></li>
        <li class="breadcrumb-item active"><a>Update User</a></li>
    </ol>
</secton>
<br />
<br />
<section class="content">
    <div class="row">
        <div class="col-md-9">
            @Html.Partial("~/Areas/Admin/Views/Shared/MessageForm.cshtml")
            <div class="box box-primary">
                @using (Html.BeginForm("UpdateUser", "User", FormMethod.Post, new { @enctype = "multipart/form-data" }))
                {
                    @Html.Partial("Partial/UserForm")
                }
            </div>

        </div>

    </div>
</section>


