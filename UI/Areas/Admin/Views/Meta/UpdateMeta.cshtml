
@{
    ViewBag.Title = "UpdateMeta";
    Layout = "~/Areas/Admin/Views/Shared/AdminLayout.cshtml";
}

<section class="content-header">


    <ol class="breadcrumb float-sm-left">
        <li class="breadcrumb-item"><a>Admin</a></li>
        <li class="breadcrumb-item"><a>Meta</a></li>
        <li class="breadcrumb-item active"><a>Update Meta</a></li>
    </ol>
    <button type="submit" class="btn btn-primary float-right" onclick="window.location.href='/Admin/Meta/MetaList'">Meta List</button>
</section>
<br/>
<br />
<section class="content">
    <div class="row">
        <div class="col-md-9">
            @Html.Partial("~/Areas/Admin/Views/Shared/MessageForm.cshtml")
            <div class="box box-primary">
                @using (Html.BeginForm("updateMeta", "Meta", FormMethod.Post, new { @enctype = "multipart/form-data" }))
                 {
                    @Html.Partial("Partial/MetaForm")
                 }
            </div>

        </div>

    </div>
</section>

